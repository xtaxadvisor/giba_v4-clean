{
  "references": [],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
  "moduleResolution": "node",
  "esModuleInterop": true,
  "resolveJsonModule": true,
  "strict": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "outDir": "./dist",
  "declaration": false,
  "emitDeclarationOnly": false,
  "jsx": "react-jsx",
  "baseUrl": ".",
  "paths": {
    "@/*": ["src/*"]
  },
  }
  ,
  "include": ["src", "src/types"],
  "exclude": ["node_modules", "dist", "dist/types"]
}